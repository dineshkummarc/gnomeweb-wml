%.html %.shtml %.php3 %.phtml: %.wml $(wildcard $(top_srcdir)/include/$(SITE)/*.wml)
	basepagename=$(basename $(notdir $<)); \
	wml $(WML_FLAGS) \
		-DURLDIR=$(urlpath) \
		-DURL=$(urlpath)/`if [ "$$basepagename" != "index" ]; then echo $$basepagename.html; fi` \
		-I$(top_srcdir)/include -I$(srcdir) $< > $@
